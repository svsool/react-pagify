{
  "name": "react-pagify",
  "description": "Simple pagination for React",
  "author": "Juho Vepsalainen <bebraw@gmail.com>",
  "version": "2.0.4",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest",
    "test:tdd": "jest --watch",
    "test:lint": "eslint . --ext .js --ext .jsx",
    "gh-pages": "webpack",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "dist": "webpack",
    "dist:min": "webpack",
    "dist:modules": "babel ./src --out-dir ./dist-modules",
    "pretest": "npm run test:lint",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy"
  },
  "main": "dist-modules",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "clean-webpack-plugin": "^0.1.7",
    "css-loader": "^0.23.1",
    "cumberbatch-name": "^1.0.9",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gh-pages": "^0.9.0",
    "git-prepush-hook": "^1.0.1",
    "highlight.js": "^9.1.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.2",
    "jest-cli": "^0.8.2",
    "json-loader": "^0.5.4",
    "purecss": "^0.6.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-ghfork": "^0.3.2",
    "remark": "^3.2.2",
    "remark-react": "^1.1.0",
    "segmentize": "^0.4.0",
    "style-loader": "^0.13.0",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.7.3"
  },
  "peerDependencies": {
    "react": ">= 0.13.* < 16.0.0",
    "lodash": ">= 3.10.* < 5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bebraw/react-pagify.git"
  },
  "homepage": "https://bebraw.github.io/react-pagify/",
  "bugs": {
    "url": "https://github.com/bebraw/react-pagify/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "pagination"
  ],
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "jsx"
    ]
  },
  "watch": {
    "test": "{src,__tests__}/*.jsx"
  },
  "pre-push": [
    "test",
    "test:lint"
  ],
  "dependencies": {}
}
